<script src="/js/require.js"></script>
<script>

require.config({
  baseUrl: '/js/vendor',
  urlArgs: 'v={{gitHash}}',
  enforceDefine: false,
  paths: {
    'chart': "/js/chart",
    'datatables.net': "/js/datatables.min"
  },
  shim: {
    chart: {
      exports: "Chart",
      deps: ['d3', 'd3-selection-multi', 'd3-scale', "datatables.net"]
    },
    "datatables.net": {
      deps: ["jquery"]
    }
  }
});

require(['chart',
  {{{typescript}}},
  'datatables.net'
], function(){});

</script>
